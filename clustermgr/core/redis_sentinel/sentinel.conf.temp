bind 127.0.0.1
port 26379
daemonize yes
pidfile "/var/run/sentinel/redis-sentinel.pid"
logfile "/var/log/redis/redis-sentinel.log"
sentinel deny-scripts-reconfig yes
dir "/var/lib/redis"
sentinel monitor mymaster 127.0.0.1 %(redis_master_node)s %(quorum)s
sentinel down-after-milliseconds mymaster 10
sentinel config-epoch mymaster 24
sentinel leader-epoch mymaster 24
protected-mode no
sentinel current-epoch 24
sentinel announce-ip 127.0.0.1
sentinel announce-port %(sentinel_node_port)s
